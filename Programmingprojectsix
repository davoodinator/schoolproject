public class Programmingprojectsix {
	public static void main (String [] args) {
		paraOne();
		paraTwo();
		paraThree();
		paraFour();
		paraFive();
	}
	public static void catText () {
		System.out.println ("Cat goes fiddle-i-fee.");
		System.out.println ();
	}
 	 public static void duckText () {
    	System.out.println ("Duck goes quack, quack,");
 	}
	public static void henText () {
		System.out.println ("Hen goes chimmy-chuck, chimmy-chuck,");
	}
	public static void gooseText () {
		System.out.println ("Goose goes hissy, hissy,");
	}
 	public static void sheepText () {
    	System.out.println ("Sheep goes baa, baa,");
    }
	public static void paraOne () {
		System.out.println ("Bought me a cat and the cat pleased me,");
		System.out.println ("I fed my cat under younder tree.");
		catText();
	}
	public static void paraTwo () {
		System.out.println ("Bought me a hen and the hen pleased me,");
		System.out.println ("I fed my hen under younder tree.");
		henText();
		catText();
	}
	public static void paraThree () {
		System.out.println ("Bought me a duck and the duck pleased me,");
		System.out.println ("I fed my duck under younder tree.");
		duckText();
		henText();
		catText();
	}
	public static void paraFour () {
		System.out.println ("Bought me a goose and the goose pleased me,");
		System.out.println ("I fed my goose under younder tree.");
		gooseText();
		duckText();
		henText();
		catText();
	}
  public static void paraFive () {
    	System.out.println ("Bought me a sheep and the sheep pleased me,");
    	System.out.println ("I fed my sheep under yonder tree.");
    	sheepText();
    	duckText();
    	henText();
    	catText();
  }	
}
